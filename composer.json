{
    "name": "vinelab/country",
    "description": "Country names and abbreviations",
    "keywords": ["laravel", "countries"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/contracts": "*",
        "illuminate/support": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Vinelab\\Country\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinelab\\Country\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vinelab\\Country\\CountryServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
